{
  "name": "whatsapp",
  "version": "1.0.0",
  "description": "",
  "main": "app/index.ts",
  "scripts": {
    "dev": "npx nodemon server.ts",
    "start": "ts-node app/index.ts",
    "build": "npm run gdb",
    "clear": "npm cache clean --force",
    "upgrade": "npx npm-check-updates -u",
    "up-whatsapp": "npx npm-check-updates -u whatsapp-web.js",
    "gdb": "npx prisma migrate dev --name create_db",
    "reset": "npx prisma migrate reset --force"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "google-spreadsheet": "^3.3.0",
    "libphonenumber-js": "^1.10.18",
    "moment": "^2.29.4",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.19.2"
  },
  "devDependencies": {
    "@types/google-spreadsheet": "^3.3.0",
    "@types/node": "^18.11.18",
    "@types/nodemon": "^1.19.2",
    "@types/qrcode-terminal": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.6.2",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "nodemonConfig": {
    "watch": [
      "app",
      "index.ts",
      "config"
    ],
    "ignore": [
      "out/*"
    ],
    "verbose": true,
    "ext": ".ts,.js,.json",
    "exec": "ts-node ./app/index.ts"
  }
}