{
  "name": "whatsapp",
  "version": "1.0.0",
  "description": "",
  "main": "src/app/index.ts",
  "scripts": {
    "dev": "npm run upgrade && ts-node-dev -r tsconfig-paths/register src/app/index.ts",
    "start": "npm run gdb && ts-node -r tsconfig-paths/register src/app/index.ts",
    "clear": "npm cache clean --force",
    "upgrade": "npx npm-check-updates -u whatsapp-web.js && npm install",
    "gdb": "npx prisma migrate dev --name create_db",
    "reset": "npx prisma migrate reset --force"
  },
  "author": "Marco Ant√¥nio",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/speech": "^5.2.0",
    "@prisma/client": "^4.9.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "google-spreadsheet": "^3.3.0",
    "googleapis": "^110.0.0",
    "libphonenumber-js": "^1.10.18",
    "moment": "^2.29.4",
    "openai": "^3.1.0",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.19.4"
  },
  "devDependencies": {
    "@types/google-spreadsheet": "^3.3.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/nodemon": "^1.19.2",
    "@types/qrcode-terminal": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.1",
    "npm-check-updates": "^16.6.2",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "nodemonConfig": {
    "watch": [
      "./src"
    ],
    "ignore": [
      "out/*"
    ],
    "verbose": true,
    "ext": ".ts,.js,.json",
    "exec": "ts-node ./src/index.ts"
  },
  "prisma": {
    "schema": "src/libs/data/prisma/schema.prisma"
  }
}